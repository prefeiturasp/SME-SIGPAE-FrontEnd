{
  "name": "terceirizadas",
  "type": "module",
  "version": "0.1.16",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run-script prettier && npm run-script eslint",
      "pre-push": "npm run-script eslint"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash -c '\"$(npm bin)/eslint\" --fix --max-warnings 0 \"$@\" || exit 0' _",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.jxs",
      "!src/reportWebVitals.jsx",
      "!src/setupTests.js",
      "!src/mocks"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "watchPathIgnorePatterns": [
      "src/components/Shareable/Avatar"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 1,
        "branches": 1,
        "functions": 1,
        "lines": 1
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/mocks/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ],
    "transform": {
      "\\.js$": [
        "babel-jest",
        {
          "configFile": "./babel.config.cjs"
        }
      ],
      "^.+\\.(ts|tsx|js|jsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "overrides": {
    "@khanacademy/react-multi-select": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "jest-watch-typeahead": "2.2.2"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^35.4.0",
    "@ckeditor/ckeditor5-react": "^5.0.6",
    "@khanacademy/react-multi-select": "^0.3.3",
    "@sentry/browser": "^7.42.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^22.15.19",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/typescript-estree": "^6.5.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "antd": "^5.22.4",
    "axios": "^1.6.2",
    "axios-mock-adapter": "^1.21.1",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "date-fns": "^2.29.3",
    "eslint-plugin-typescript": "^0.14.0",
    "file-saver": "^2.0.5",
    "final-form": "^4.20.9",
    "final-form-arrays": "^3.0.2",
    "final-form-calculate": "^1.3.2",
    "html-to-react": "^1.5.1",
    "http-status-codes": "^2.2.0",
    "jquery.easing": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "locutus": "^2.0.16",
    "moment": "^2.29.4",
    "multiselect-react-dropdown": "^2.0.25",
    "patch-package": "^7.0.1",
    "ramda": "^0.28.0",
    "react": "^18.3.1",
    "react-big-calendar": "^1.8.1",
    "react-bootstrap": "^2.9.1",
    "react-chartjs-2": "^5.2.0",
    "react-collapse": "^5.1.1",
    "react-datepicker": "4.16.0",
    "react-dom": "^18.3.1",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.21.2",
    "react-select": "^5.8.0",
    "react-text-mask": "^5.4.3",
    "react-toastify": "^9.1.2",
    "redux": "^4.2.1",
    "redux-form": "^8.3.9",
    "redux-form-input-masks": "^2.0.2",
    "redux-multi": "^0.1.12",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.2",
    "startbootstrap-sb-admin-2": "^4.1.4",
    "typescript": "^4.9.5",
    "uuid": "^11.1.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "eject": "vite eject",
    "eslint": "eslint ./src/**/**/**/**/*.{jsx,js,tsx,ts}",
    "prettier": "prettier  --check src/**/**/**/*.{jsx,js,scss,tsx,ts}",
    "prettier-fix": "prettier  --write src/**/**/**/*.{jsx,js,scss,tsx,ts}",
    "coverage": "jest --coverage",
    "postinstall": "patch-package",
    "prepare": "husky install",
    "jest-preview": "jest-preview",
    "lint": "NODE_OPTIONS=--max_old_space_size=4096 eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "NODE_OPTIONS=--max_old_space_size=4096 eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.5.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preview": "^0.3.1",
    "lint-staged": "^12.5.0",
    "msw": "^0.35.0",
    "node-localstorage": "^1.3.1",
    "prettier": "^2.8.8",
    "redux-mock-store": "^1.5.5",
    "sass": "^1.83.0",
    "ts-jest": "^29.3.4",
    "vite": "^6.3.5",
    "vite-plugin-environment": "^1.1.3",
    "whatwg-fetch": "^3.6.20"
  },
  "setupFiles": [
    "./src/setupTests.js"
  ]
}
